generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Statistics {
  id Int @id @default(autoincrement())
  action String 
  date DateTime
  shop_id Int
  plu String
  raw_log String
}